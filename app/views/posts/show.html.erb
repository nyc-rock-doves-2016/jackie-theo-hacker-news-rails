<h1><%= @post.title %></h1>
<h2><%= @post.created_at %></h2>
<% if @post.user %>
<h2><a href='<%=user_path(@post.user)%>'><%= @post.user.full_name %></a></h2>
<% end %>
<h2><%= @post.body %></h2>
<h2>Comments:</h2>
<ul>
  <% @comments.each do |comment| %>
  <li><%=comment.content%></li>
  <%end%>
</ul>
<%= link_to 'New Comment', new_post_comment_path(@post) %>
